<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <ui:style>
  .actions {
    padding: 0.5em;
    line-height: 150%;
  }

  .item {
    display: block;
  }
  </ui:style>

  <g:FlowPanel styleName='{style.actions}'>
    <g:Button styleName='{style.item}' ui:field='addDocumentButton'>Add Document</g:Button>
  </g:FlowPanel>
</ui:UiBinder>
