<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <ui:style>
  .anchor {
    margin: 0 8px;
  }
  </ui:style>

  <g:HTMLPanel>
    <g:Anchor styleName='{style.anchor}' ui:field='previousButton' href='javascript:;'>
      &lt; previous
    </g:Anchor>
    <span ui:field='countLabel'/>
    <g:Anchor styleName='{style.anchor}' ui:field='nextButton' href='javascript:;'>
      next &gt;
    </g:Anchor>
  </g:HTMLPanel>
</ui:UiBinder>
